{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "node_modules/pg/**",
                    "node_modules/pg-hstore/**",
                    "node_modules/pg-protocol/**",
                    "node_modules/pg-pool/**"
                ]
            }
        }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
}